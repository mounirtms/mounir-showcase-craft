{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "dev:debug": "vite --debug",
    "build": "npm run type-check && vite build",
    "build:prod": "npm run type-check && vite build --mode production",
    "build:analyze": "npm run build && npm run bundle:analyze",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "start": "vite --host",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:cache": "eslint . --ext .ts,.tsx --cache",
    "lint:report": "eslint . --ext .ts,.tsx --format html --output-file eslint-report.html",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:staged": "prettier --write",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run",
    "test:integration": "vitest run src/test/integration/**/*.test.tsx",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "playwright test src/test/visual/",
    "test:visual:update": "playwright test src/test/visual/ --update-snapshots",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:runner": "node scripts/test-runner.js",
    "test:runner:unit": "node scripts/test-runner.js unit",
    "test:runner:integration": "node scripts/test-runner.js integration",
    "test:runner:e2e": "node scripts/test-runner.js e2e",
    "test:runner:visual": "node scripts/test-runner.js visual",
    "test:runner:coverage": "node scripts/test-runner.js coverage",
    "clean": "rimraf dist node_modules/.vite .eslintcache",
    "clean:all": "rimraf dist node_modules package-lock.json .eslintcache && npm install",
    "clean:cache": "rimraf node_modules/.vite .eslintcache",
    "deploy": "npm run build:prod && gh-pages -d dist",
    "deploy:preview": "npm run build && npm run preview",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --upload.target=temporary-public-storage",
    "performance": "node scripts/performance-monitor.js",
    "bundle:analyze": "node scripts/bundle-analyzer.js",
    "bundle:size": "node scripts/bundle-analyzer.js",
    "perf:monitor": "npm run performance && npm run bundle:analyze",
    "perf:ci": "npm run lighthouse:ci && npm run bundle:size",
    "perf:enforce": "node scripts/performance-enforcer.js",
    "perf:full": "npm run build && npm run perf:monitor && npm run perf:enforce",
    "quality": "npm run lint:check && npm run format:check && npm run type-check",
    "quality:fix": "npm run lint:fix && npm run format",
    "pre-commit": "npm run quality",
    "pre-push": "npm run quality && npm run build",
    "generate:component": "node scripts/generate-component.js",
    "generate:page": "node scripts/generate-page.js",
    "generate:hook": "node scripts/generate-hook.js",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:audit": "npm audit",
    "deps:fix": "npm audit fix",
    "dev:full": "npm run clean:cache && npm run dev",
    "build:full": "npm run clean && npm run quality && npm run build:prod",
    "ci": "npm run quality && npm run build && npm run perf:ci",
    "setup": "node scripts/setup-dev.js",
    "info": "node scripts/dev-utils.js info",
    "check": "node scripts/dev-utils.js check",
    "stats": "node scripts/dev-utils.js stats",
    "utils": "node scripts/dev-utils.js help"
  },
