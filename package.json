{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://mounir1.github.io",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "dev:debug": "vite --debug",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "build:analyze": "npm run build && npm run bundle:analyze",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "start": "vite --host",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "clean": "rimraf dist node_modules/.vite .eslintcache",
    "clean:all": "rimraf dist node_modules package-lock.json .eslintcache",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d dist -t true",
    "deploy:ci": "npm run build:prod && gh-pages -d dist -t true -r https://$GITHUB_TOKEN@github.com/mounir1/mounir1.github.io.git",
    "deploy:preview": "npm run build && npm run preview",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --upload.target=temporary-public-storage",
    "bundle:analyze": "vite-bundle-analyzer",
    "perf:monitor": "npm run lighthouse",
    "perf:full": "npm run build && npm run perf:monitor",
    "quality": "npm run lint && npm run format:check && npm run type-check",
    "prepare": "husky install"
  },
